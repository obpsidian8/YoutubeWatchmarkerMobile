{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "browser_style": false,
      "default_icon": {
         "128": "content/icon.png"
      },
      "default_title": "Watchmarker Mobile",
      "default_popup": "/content/index.html"
   },
   "content_scripts": [ {
      "css": [ "youtube.css" ],
      "js": ["youtube.js"],
      "matches": [ "*://*.youtube.com/*", "*://m.youtube.com/*" ]
   } ],
   "description": "Automatically mark videos on Youtube mobile and desktop that you have already watched.",
   "homepage_url": "https://github.com/obpsidian8/YoutubeWatchmarkerMobile",
   "icons": {
      "128": "content/icon.png"
   },
   "manifest_version": 2,
   "name": "Watchmarker Mobile",
   "permissions": [ "alarms","storage", "downloads", "history", "tabs", "cookies", "webRequest", "webRequestBlocking", "*://www.youtube.com/*", "*://*.ytimg.com/vi/*/*", "*://m.youtube.com/*"],
   "version": "1.6.9"
}